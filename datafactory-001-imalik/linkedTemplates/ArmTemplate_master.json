{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-001-imalik"
		},
		"Ishfaq0602DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Ishfaq0602DataLake'"
		},
		"ishfaq0602Storerage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ishfaq0602Storerage'"
		},
		"datalake0602_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake0602'"
		},
		"dataworkspace_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dataworkspace'"
		},
		"Ishfaq0602DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ishfaq0602.dfs.core.windows.net/"
		},
		"ishfaq0602Storerage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ishfaq0602.dfs.core.windows.net/"
		},
		"datalake0602_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake060211.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-001-imalik_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ishfaq0602DataLake_accountKey": {
						"value": "[parameters('Ishfaq0602DataLake_accountKey')]"
					},
					"ishfaq0602Storerage_accountKey": {
						"value": "[parameters('ishfaq0602Storerage_accountKey')]"
					},
					"datalake0602_accountKey": {
						"value": "[parameters('datalake0602_accountKey')]"
					},
					"dataworkspace_connectionString": {
						"value": "[parameters('dataworkspace_connectionString')]"
					},
					"Ishfaq0602DataLake_properties_typeProperties_url": {
						"value": "[parameters('Ishfaq0602DataLake_properties_typeProperties_url')]"
					},
					"ishfaq0602Storerage_properties_typeProperties_url": {
						"value": "[parameters('ishfaq0602Storerage_properties_typeProperties_url')]"
					},
					"datalake0602_properties_typeProperties_url": {
						"value": "[parameters('datalake0602_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}